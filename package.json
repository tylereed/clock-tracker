{
  "name": "clock-tracker",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "antlr4-generate": "antlr4 -Dlanguage=TypeScript -lib grammars -o src/generated/parsers grammars/Roll.g4 grammars/Attack.g4"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@vueuse/core": "^13.5.0",
    "antlr4": "4.13.2",
    "debounce": "^2.2.0",
    "luxon": "^3.7.1",
    "pinia": "^3.0.3",
    "pluralize": "^8.0.0",
    "roboto-fontface": "0.10.0",
    "typo-js": "^1.2.5",
    "vue": "^3.5.17",
    "vue-toast-notification": "^3.1.3",
    "vuetify": "^3.9.0"
  },
  "devDependencies": {
    "@babel/types": "^7.28.1",
    "@pinia/testing": "^1.0.2",
    "@types/luxon": "^3.6.2",
    "@types/node": "^24.0.13",
    "@types/pluralize": "^0.0.33",
    "@types/typo-js": "^1.2.2",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "canvas": "^3.1.2",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^26.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^5.8.3",
    "unplugin-fonts": "^1.3.1",
    "vite": "^6.3.5",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.1"
  }
}

{
  "name": "clock-tracker",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "antlr4-generate": "antlr4 -Dlanguage=TypeScript -lib grammars -o src/generated/parsers grammars/Roll.g4 grammars/Attack.g4"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@vueuse/core": "^12.4.0",
    "antlr4": "4.13.2",
    "debounce": "^2.2.0",
    "luxon": "^3.5.0",
    "pluralize": "^8.0.0",
    "roboto-fontface": "0.10.0",
    "vue": "^3.5.13",
    "vue-toast-notification": "^3.1.3",
    "vuetify": "^3.7.6"
  },
  "devDependencies": {
    "@babel/types": "^7.26.5",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.7",
    "@types/pluralize": "^0.0.33",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "canvas": "^3.0.1",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^5.7.3",
    "unplugin-fonts": "^1.3.1",
    "vite": "^6.0.7",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^3.0.0",
    "vue-tsc": "^2.2.0"
  }
}
